{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/projects",
      "dest": "/projects.html"
    },
    {
      "src": "/resume",
      "dest": "/resume.html"
    },
    {
      "src": "/awards",
      "dest": "/awards.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}